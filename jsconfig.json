// 这里贴出我的jsconfig配置，和上面的插件配置对应
{
    "compilerOptions": {
        "target": "ES2016",
        "module": "ES6",
        "allowSyntheticDefaultImports": true,
        "baseUrl": "./",
        "paths": {
            "@*": ["src/*"],
            "@css*": ["src/assets/css*"],
            "@icon*": ["src/assets/icon/*"],
            "@image*": ["src/assets/image/*"]
        }
    }
}